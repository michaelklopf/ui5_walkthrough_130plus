<mvc:View
   controllerName="mk.tutorial.controller.App"
   xmlns="sap.m"
   xmlns:mvc="sap.ui.core.mvc"
   displayBlock="true">
   <App
     class="myAppDemoWT">
     <pages>
        <Page
          title="{i18n>homePageTitle}">
          <headerContent>
            <Button
              icon="sap-icon://hello-world"
              press="onOpenDialog" />
          </headerContent>
          <content>
            <mvc:XMLView viewName="mk.tutorial.view.WelcomePanel" />
            <mvc:XMLView viewName="mk.tutorial.view.InvoiceList" />
          </content>
        </Page>
     </pages>
   </App>
</mvc:View>
